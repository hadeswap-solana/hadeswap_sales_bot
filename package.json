{
  "type": "module",
  "engines": {
    "node": "16.15.0"
  },
  "scripts": {
    "start": "node -r dotenv/config ./lib/index.js",
    "postinstall": "tsc --build && npm run copy-font && npm run copy-font-bold && npm run copy-hadeswap-logo && npm run copy-solana-logo",
    "copy-font": "cp ./src/generators/fonts/Poppins.ttf ./lib/generators/fonts/Poppins.ttf",
    "copy-font-bold": "cp ./src/generators/fonts/PoppinsBold.ttf ./lib/generators/fonts/PoppinsBold.ttf",
    "copy-hadeswap-logo": "cp ./src/generators/input/hadeswap_logo.png ./lib/generators/input/hadeswap_logo.png",
    "copy-solana-logo": "cp ./src/generators/input/solana.png ./lib/generators/input/solana.png"
  },
  "dependencies": {
    "@metaplex-foundation/js": "^0.16.1",
    "@solana/web3.js": "^1.63.1",
    "@types/express": "^4.17.14",
    "@types/node": "^18.8.3",
    "@types/oauth": "^0.9.1",
    "@types/socket.io": "^3.0.2",
    "@types/twit": "^2.2.30",
    "body-parser": "^1.20.1",
    "canvas": "^2.10.1",
    "console-stamp": "^3.0.6",
    "discord.js": "^14.6.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "hadeswap-sdk": "^0.3.10",
    "oauth": "^0.10.0",
    "socket.io": "^4.5.2",
    "twit": "^2.2.11",
    "typescript": "^4.7.3"
  }
}
