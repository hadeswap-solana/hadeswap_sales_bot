{
  "compilerOptions": {
    "target": "es2022",
    "lib": [],
    "module": "es2022",
    "allowJs": true,
    "skipLibCheck": true,
    "useUnknownInCatchVariables": false,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": false,
    "outDir": "lib",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "src",
    "src/generators/input/hadeswap_logo.png",
    "src/generators/input/solana.png",
    "src/generators/fonts/Poppins.ttf",
    "src/generators/fonts/PoppinsBold.ttf",
    "src/generators/output"
  ],
  "exclude": ["node_modules"]
}